<% if user_signed_in? %>
    <% @user.each do |user| %>
        <%= user.name %>
        <%= user.balance %><br>
    <% end %>
    <%= pie_chart User.group(:balance).count %>

    <%= area_chart Bill.group_by_week(:created_at).maximum(:value) %>

    <%= bar_chart Market.group(:name).sum(:count) %>

<% end %>


